class ShareDataset_EntityTypes_Enum(Enum):
    CAN_SHARE = 'CAN_SHARE'
    CAN_EDIT = 'CAN_EDIT'
    CO_OWNER = 'CO_OWNER'


def generate_share_dataset_payload(entity_type,  # USER or GROUP

                                   entity_id,
                                   access_level
                                   ):

    return {
        "permissions": [
            {
                "type": entity_type,
                "id": entity_id,
                "accessLevel": access_level
            }
        ],
        "sendEmail": true
    }
